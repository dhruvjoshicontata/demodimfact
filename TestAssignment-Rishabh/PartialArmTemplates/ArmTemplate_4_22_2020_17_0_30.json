{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "TestAssignment-Rishabh"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/Test_Assignment_Rishabh_Pipeline')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy data TestCopy",
						"description": "some description here",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "AzureSqlSource",
								"sqlReaderQuery": "Select FirstName, LastName, Age, DOB, Email, GoodToImport FROM TestCopy",
								"queryTimeout": "02:00:00"
							},
							"sink": {
								"type": "AzureSqlSink",
								"writeBatchSize": 100,
								"sqlWriterStoredProcedureName": "[[dbo].[CopyAssignment_Rishabh]",
								"sqlWriterTableType": "TestCopyType",
								"storedProcedureTableTypeParameterName": "TestCopytype",
								"disableMetricsCollection": false
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "FirstName",
											"type": "String"
										},
										"sink": {
											"name": "Firstname",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "LastName",
											"type": "String"
										},
										"sink": {
											"name": "Lastname",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "Age",
											"type": "Int32"
										},
										"sink": {
											"name": "Age",
											"type": "Int32"
										}
									},
									{
										"source": {
											"name": "DOB",
											"type": "DateTime"
										},
										"sink": {
											"name": "Dob",
											"type": "DateTime"
										}
									},
									{
										"source": {
											"name": "Email",
											"type": "String"
										},
										"sink": {
											"name": "Email",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "GoodToImport",
											"type": "Boolean"
										},
										"sink": {
											"name": "GoodToImport",
											"type": "Boolean"
										}
									}
								]
							}
						},
						"inputs": [
							{
								"referenceName": "TestCopy_Source_DataSet",
								"type": "DatasetReference",
								"parameters": {
									"DBName": "Traniee_Assignment",
									"ServerName": "contata.database.windows.net"
								}
							}
						],
						"outputs": [
							{
								"referenceName": "TestCopy_Rishabh_DataSet",
								"type": "DatasetReference",
								"parameters": {
									"DBName": "Traniee_Assignment",
									"ServerName": "contata.database.windows.net"
								}
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": []
		}
	]
}